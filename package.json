  "name": "atlas-angular2",
  "version": "1.0.0",
  "scripts": {
    "start-ui": "ng serve --deploy-url= --base-href=. --open",
    "start-ui-hmr": "ng serve --configuration hmr --deploy-url= --base-href=. --open",
    "dev": "concurrently \"npm run start-ui\" \"npm run server-dev\" ",
    "build:brod": "ng build --prod",
    "start": "concurrently   \"npm run server-dev\" \"npm run tsc-be:w\"  \"npm run start-ui\" ",
    "server": "tsc --project tsconfig-be.json && node server/index.js",
    "server-dev": "concurrently \"npm run tsc-be:w\" \"npm run node-dev\" ",
    "lite": "lite-server",
    "node-dev": "node-dev server/index.js",
    "tsc-be:w": "tsc -w --project tsconfig-be.json",
    "typings": "typings"
  },
