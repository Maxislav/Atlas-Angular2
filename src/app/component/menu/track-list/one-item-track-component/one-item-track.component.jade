.title
    .color([style.background]="track.color")
    .info
        .date {{track.date | date: 'dd/MM/yyyy'}}
        .dist   {{track.distance}} km
    .del( (click)='hideTrack()') &#215;
.action
    .download((click)="track.download()")
    .save((click)="saveChange()")
    .route-points((click)="track.showSrcPoint()")
    .go( (click)="onGo(track)") Play &#9658;
